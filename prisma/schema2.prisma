generator client {
  provider = "prisma-client-js"
  output   = "./generated/client2"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model ScopeNoSql {
  id   String  @id @default(auto()) @map("_id") @db.ObjectId
  name String
  desc String?

  @@index([id])
  @@map("scope")
}
