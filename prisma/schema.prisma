generator client {
  provider = "prisma-client-js"
  output   = "./generated/client1"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ScopeSql {
  id   Int     @id @default(autoincrement())
  name String
  desc String?

  @@index([id])
  @@map("scope")
}

model ResourceSql {
  id   Int     @id @default(autoincrement())
  name String
  desc String?

  @@index([id])
  @@map("resource")
}
